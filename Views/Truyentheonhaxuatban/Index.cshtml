@model IEnumerable<MangaStore.Models.MAQUANHE>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div style="margin-left: 200px; margin-top : 200px;">
    <h2 style="color: red;">QUẢN LÝ TRUYỆN NỔI BẬT</h2>

    <p>
        @Html.ActionLink("THÊM MỚI", "Create")
    </p>
    <table class="table">


        <tr>
            <th>
                ID NHÀ XUẤT BẢN
            </th>

            <th>
                MÃ NHÀ XUẤT BẢN
            </th>
            <th>
                MÃ TRUYỆN
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MaQH)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaNXB)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaTruyen)
                </td>
                <td>
                    @Html.ActionLink("Sửa", "Edit", new { id = item.MaQH }) |
                    @Html.ActionLink("Xóa", "Delete", new { id = item.MaQH })
                </td>
            </tr>
        }

    </table>
</div>



